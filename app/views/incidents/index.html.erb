<p id="notice"><%= notice %></p>

<h1>Listing Incidents</h1>

<table>
  <thead>
    <tr>
      <th>Incidentnum</th>
      <th>Incidentdatetime</th>
      <th>Permitnum</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incidents.each do |incident| %>
      <tr>
        <td><%= incident.id %></td>
        <td><%= incident.incidentDateTime %></td>
        <td><%= incident.permitNum %></td>
        <td><%= incident.address %></td>
        <td><%= incident.city %></td>
        <td><%= incident.state %></td>
        <td><%= incident.zip %></td>
        <td><%= link_to 'Show', incident %></td>
        <td><%= link_to 'Edit', edit_incident_path(incident) %></td>
        <td><%= link_to 'Destroy', incident, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incident', new_incident_path %>
